{
  "name": "picofeed",
  "version": "5.0.0",
  "description": "Microscopic Secure Feed occupying a single buffer",
  "type": "module",
  "main": "index.js",
  "files": [
    "lib",
    "bin",
    "index.js"
  ],
  "scripts": {
    "test": "node test.js",
    "debug": "node inspect test.js",
    "cov": "c8 --check-coverage --lines=100 node test.js",
    "size": "esbuild --bundle --minify --format=esm index.js --outfile=/dev/null --analyze",
    "lint": "standard",
    "prepublishOnly": "standard --fix && npm run cov"
  },
  "dependencies": {
    "@noble/curves": "^1.2.0",
    "@noble/hashes": "^1.3.0"
  },
  "devDependencies": {
    "brittle": "^3.2.1",
    "c8": "^8.0.1",
    "esbuild": "^0.19.5",
    "standard": "^17.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/telamon/picofeed.git"
  },
  "keywords": [
    "space efficient",
    "secure",
    "feed",
    "transport",
    "decentralized",
    "p2p",
    "block",
    "chain"
  ],
  "author": {
    "name": "Tony Ivanov",
    "email": "telamohn@pm.me"
  },
  "license": "AGPL-3.0-or-later",
  "bugs": {
    "url": "https://github.com/telamon/picofeed/issues"
  },
  "homepage": "https://github.com/telamon/picofeed#readme"
}
